{
  "name": "spryker/sales-merchant-portal-gui",
  "type": "library",
  "description": "SalesMerchantPortalGui module",
  "license": "proprietary",
  "require": {
    "php": ">=7.2",
    "spryker/dashboard-merchant-portal-gui-extension": "^0.1.0",
    "spryker/merchant": "^3.0.0",
    "spryker/merchant-sales-order": "^0.7.0",
    "spryker/merchant-user": "^0.5.0",
    "spryker/kernel": "^3.30.0",
    "spryker/propel-orm": "^1.0.0",
    "spryker/sales": "^11.0.0",
    "spryker/router": "^1.1.0",
    "spryker/twig": "^3.0.0"
  },
  "require-dev": {
    "spryker/code-sniffer": "*"
  },
  "autoload": {
    "psr-4": {
      "Spryker\\": "src/Spryker/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "scripts": {
    "cs-check": "phpcs -p -s --standard=vendor/spryker/code-sniffer/Spryker/ruleset.xml src/",
    "cs-fix": "phpcbf -p --standard=vendor/spryker/code-sniffer/Spryker/ruleset.xml src/"
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0.x-dev"
    }
  },
  "config": {
    "sort-packages": true
  }
}
